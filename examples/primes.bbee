mut i = 1,
for (i = i + 1, i < 100) {
    mut isPrime = 1,
    
    mut j = 1,
    for (j = j + 1, j < i - 1 && isPrime) {
        if ((i % j) == 0) {
            isPrime = 0
        }
    },
    
    if isPrime {println@("is  prime: " + stringify@i)}
    else {println@("not prime: " + stringify@i)}
}